srcdir = @srcdir@
top_srcdir = @top_srcdir@
builddir = @builddir@
top_builddir = ./@top_builddir@

DIR = src tool test example doc dist

default all libs programs examples tests install uninstall clean distclean depend: Makefile
	@if test "$@" != "distclean"; then \
	  if test -f "config/Makefile"; then $(MAKE) -C config $@; fi; \
	fi
	@dirs='$(DIR)'; for d in $$dirs; do \
	  if test -f "$$d/Makefile"; then \
	    $(MAKE) -C $$d $@; \
	    if test "$$?" != 0; then break; fi; \
	  fi; \
	done
	@if test "$@" = "distclean"; then \
	  if test -f "config/Makefile"; then $(MAKE) -C config $@; fi; \
	  $(MAKE) distclean-top; \
	fi
distclean-top:
	-rm -f Makefile config.log config.status devel/Makefile
Makefile: $(srcdir)/Makefile.in
	(cd $(top_builddir) && ./config.status)
bootstrap:
	autoconf --force
	rm -rf autom4te*.cache
.PHONY: bootstrap
